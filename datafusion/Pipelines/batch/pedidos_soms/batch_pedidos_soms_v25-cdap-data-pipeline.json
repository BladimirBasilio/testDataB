                    },
                    "properties": {
                        "jdbcPluginType": "jdbc",
                        "enableAutoCommit": "false",
                        "connectionString": "${db_url}",
                        "jdbcPluginName": "cloudsql-postgresql",
                        "user": "${db_username}",
                        "password": "${db_password}",
                        "query": "WITH ps AS (\n\tSELECT *\n\tFROM staging.pedidos_soms\n), rem AS (\n\tINSERT INTO clientes.clientes (id_cliente, primer_nombre)\n\tSELECT\n\t\tps.id_cliente_remitente, ps.nombre_remitente\n\tFROM\n\t\tps\n\tON CONFLICT ON CONSTRAINT pk_clientes\n\tDO NOTHING\n), cel_rem AS (\n\tINSERT INTO clientes.cliente_telefonos_de_contacto(\n\t\tid_cliente_telefonos_de_contacto, id_cliente, telefono, alias_telefono)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_telefonos_de_contacto_pk'::regclass), ps.id_cliente_remitente,\n\t\tps.telefono_celular_remitente, 'CELULAR'\n\tFROM\n\t\tps\n), fij_rem AS (\n\tINSERT INTO clientes.cliente_telefonos_de_contacto(\n\t\tid_cliente_telefonos_de_contacto, id_cliente, telefono, alias_telefono)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_telefonos_de_contacto_pk'::regclass), ps.id_cliente_remitente,\n\t\tps.telefono_fijo_remitente, 'CASA'\n\tFROM\n\t\tps\n), tra_rem AS (\n\tINSERT INTO clientes.cliente_telefonos_de_contacto(\n\t\tid_cliente_telefonos_de_contacto, id_cliente, telefono, alias_telefono)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_telefonos_de_contacto_pk'::regclass), ps.id_cliente_remitente,\n\t\tps.telefono_oficina_remitente, 'TRABAJO'\n\tFROM\n\t\tps\n), mai_rem AS (\n\tINSERT INTO clientes.cliente_emails_de_contacto(\n\t\tid_cliente_emails_de_contacto, id_cliente, email, tipo)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_emails_de_contacto_pk'::regclass), ps.id_cliente_remitente,\n\t\tps.correo_electronico_remitente, 'PERSONAL'\n\tFROM\n\t\tps\n), des AS (\n\tINSERT INTO clientes.clientes (id_cliente, primer_nombre, apellido_paterno, apellido_materno)\n\tSELECT\n\t\tps.id_cliente_destinatario, ps.nombre_destinatario, ps.apellido_paterno_destinatario, ps.apellido_materno_destinatario\n\tFROM\n\t\tps\n\tON CONFLICT ON CONSTRAINT pk_clientes\n\tDO NOTHING\n), cel_des AS (\n\tINSERT INTO clientes.cliente_telefonos_de_contacto(\n\t\tid_cliente_telefonos_de_contacto, id_cliente, telefono, alias_telefono)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_telefonos_de_contacto_pk'::regclass), ps.id_cliente_destinatario,\n\t\tps.telefono_celular_destinatario, 'CELULAR'\n\tFROM\n\t\tps\n), fij_des AS (\n\tINSERT INTO clientes.cliente_telefonos_de_contacto(\n\t\tid_cliente_telefonos_de_contacto, id_cliente, telefono, alias_telefono)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_telefonos_de_contacto_pk'::regclass), ps.id_cliente_destinatario,\n\t\tps.telefono_fijo_destinatario, 'CASA'\n\tFROM\n\t\tps\n), tra_des AS (\n\tINSERT INTO clientes.cliente_telefonos_de_contacto(\n\t\tid_cliente_telefonos_de_contacto, id_cliente, telefono, alias_telefono)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_telefonos_de_contacto_pk'::regclass), ps.id_cliente_destinatario,\n\t\tps.telefono_oficina_destinatario, 'TRABAJO'\n\tFROM\n\t\tps\n), mai_des AS (\n\tINSERT INTO clientes.cliente_emails_de_contacto(\n\t\tid_cliente_emails_de_contacto, id_cliente, email, tipo)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_emails_de_contacto_pk'::regclass), ps.id_cliente_destinatario,\n\t\tps.correo_electronico_destinatario, 'PERSONAL'\n\tFROM\n\t\tps\n), dir AS (\n\tINSERT INTO clientes.cliente_direcciones (id_direccion_del_cliente, \n\t\tid_cliente, calle, numero_exterior, numero_interior, codigo_postal,  \n\t\tid_colonia, id_municipio, id_estado_del_pais, entre_calle_1, entre_calle_2)\n\tSELECT\n\t\tnextval('clientes.seq_cliente_direcciones_pk'::regclass), \n\t\tps.id_cliente_destinatario, ps.calle_destinatario, ps.numero_exterior_destinatario, ps.numero_interior_destinatario,\n\t\tps.codigo_postal_destinatario, col.id_catalogo, mpo.id_catalogo, edo.id_catalogo,\n\t\tps.entre_calle_destinatario, ps.y_calle_destinatario\n\tFROM\n\t\tps\n\tINNER JOIN shared.catalogos col ON col.descripcion_catalogo = ps.colonia_destinatario\n\tINNER JOIN shared.catalogos mpo ON mpo.descripcion_catalogo = ps.municipio_destinatario\n\tINNER JOIN shared.catalogos edo ON edo.descripcion_catalogo = ps.estado_destinatario\n\tRETURNING id_direccion_del_cliente, id_cliente\n), ped AS (\n\tINSERT INTO pedidos.pedidos(numero_del_documento, estado_renglon_bitacora, fecha_modificacion, hora_modificacion, \n\t\t\t\t\t\t\t\tfecha_emision, hora_emision, observaciones, id_tipo_entrega, \n\t\t\t\t\t\t\t\tid_cliente_remitente, id_cliente_destinatario, id_direcciones_destinatario, id_tipo_de_act, \n\t\t\t\t\t\t\t\tid_tipo_de_documento, id_periodicidad, id_tienda_y_locacion_destino, id_tienda_y_locacion_surte,\n\t\t\t\t\t\t\t\tfecha_de_compra, id_frecuencia_visita, orden_original,estado_orden)\n\tSELECT DISTINCT\n\t\tps.numero_del_documento, ps.estado_por_linea, ps.fecha_actualizacion_estado, ps.hora_actualizacion_estado, \n\t\tps.fecha_emision, ps.hora_emision, ps.observaciones, ent.id_catalogo,\n\t\tps.id_cliente_remitente, ps.id_cliente_destinatario, dir.id_direccion_del_cliente, act.id_catalogo,\n\t\tdoc.id_catalogo, per.id_catalogo, ps.locacion_destino, ps.locacion_que_surte,\n\t\tps.fecha_de_compra, fre.id_catalogo, ps.orden_original, ''\n\tFROM ps\n\tINNER JOIN dir ON ps.id_cliente_destinatario = dir.id_cliente\n\tINNER JOIN shared.catalogos ent ON ent.descripcion_catalogo = ps.id_tipo_entrega\n\tINNER JOIN shared.catalogos act ON act.descripcion_catalogo = ps.id_tipo_act\n\tINNER JOIN shared.catalogos doc ON doc.descripcion_catalogo = ps.id_tipo_documento\n\tINNER JOIN shared.catalogos per ON per.descripcion_catalogo = ps.periodicidad\n\tINNER JOIN shared.catalogos fre ON fre.descripcion_catalogo = ps.frecuencia_visita\n)\nINSERT INTO pedidos.pedidos_detalle_sku(\n\tid_locacion_surte, fecha_recalculada, indicador_marketplace, cantidad_entregada, \n\tfecha_de_surtido, cantidad_recogida_orden_de_venta, hora_actualizacion_estado, \n\tnumero_de_intentos_de_entrega, fecha_actualizacion_estado, usuario_actualizador_de_estado, \n\tproveedor_de_mensajeria, fecha_rango_promesa_de_entrega, piezas, id_causa_de_noentrega, \n\tfecha_real_de_entrega, cantidad_cancelada, numero_de_guia)\n\tSELECT ps.locacion_que_surte, ps.fecha_recalculada, ps.indicador_mkp, ps.cantidad_entregada,\n\tps.fecha_surtido, ps.cantidad_recogida, ps.hora_actualizacion_estado, ps.numero_de_intentos_de_entrega,\n\tps.fecha_actualizacion_estado, ps.usuario_actualizador_estado, ps.proveedor_de_mensajeria,\n\tps.fecha_rango_promesa_entrega, ps.piezas, nen.id_catalogo, ps.fecha_real_entrega,\n\tps.cantidad_cancelada, ps.numero_de_guia\n\tFROM ps\n\tINNER JOIN shared.catalogos nen ON nen.descripcion_catalogo = ps.causa_no_entrega\n",
                        "connectionArguments": "currentSchema=staging"
                    }
                },
                "outputSchema": [
                    {
                        "name": "etlSchemaBody",
                        "schema": ""
                    }
                ],
                "id": "DBTransform"
            },
